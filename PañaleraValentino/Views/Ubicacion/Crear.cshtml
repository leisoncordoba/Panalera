@model PañaleraValentino.Models.Ubicacion
@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Crear</h2>

<div>
    <form action="/Ubicacion/Guardar" method="post">
        <div class="form-group">
            <label for="idubicacion">IdUbicacion: </label>
            <input type="text" class="form-control" name="IdUbicacion" id="IdUbicacion" />
        </div>
        <div class="form-group">
            <label for="nombre">Nombre: </label>
            <input type="text" class="form-control" name="Nombre" id="Nombre" />

        </div>

        <div class="form-group">
            <label for="area">Area: </label>
            <input type="text" class="form-control" name="Area" id="Area" />

        </div>


        <div class="form-group">
            <p><button class="btn btn-primary btn-lg" type="submit" role="button">Guardar</button></p>
        </div>
    </form>

</div>
