@model PañaleraValentino.Models.Ubicacion
@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles</h2>

<div>
    <form action="/Ubicacion/Modificar" method="post">
        <div class="form-group">

            <label for="idubicacion">IdUbicacion: </label>
            <label for="idubicacion" name="IdUbicacion">@Model.IdUbicacion</label>
            <input type="hidden" name="IdUbicacion" value="@Model.IdUbicacion" />
        </div>
        <div class="form-group">
            <label for="nombre">Nombre: </label>
            <label for="nombre" name="Nombre">@Model.Nombre</label>
            <input type="hidden" name="Nombre" value="@Model.Nombre" />

        </div>

        <div class="form-group">
            <label for="area">Area: </label>
            <label for="area" name="Area">@Model.Area</label>
            <input type="hidden" name="Area" value="@Model.Area" />

        </div>



        <div class="form-group">
            <br />
            <a class="btn btn-primary btn-lg" href="/Ubicacion/Index" role="button">Regresar</a>

        </div>


    </form>





</div>

