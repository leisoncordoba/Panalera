@model  PañaleraValentino.Models.Ubicacion
@{
    ViewBag.Title = "Editar";
    Layout="~/Views/Shared/_Layout.cshtml";
}

<h2>Editar</h2>

<div>
    <form action="/Ubicacion/Modificar" method="post">
        <div class="form-group">
            <label for="idubicacion">IdUbicacion: </label>
            <label for="idubicacion" name="IdUbicacion">@Model.IdUbicacion</label>
            <input type="hidden" name="IdUbicacion" value="@Model.IdUbicacion" />

        </div>
        <div class="form-group">
            <label for="nombre">Nombre: </label>
            <input type="text" class="form-control" name="Nombre" value="@Model.Nombre" />
        </div>

        <div class="form-group">
            <label for="area">Area: </label>
            <input type="text" class="form-control" name="Area" value="@Model.Area" />
        </div>



        <div class="form-group">
            <p><button class="btn btn-primary btn-lg" type="submit" role="button">Guardar</button></p>
            <br />
            <a class="btn btn-primary btn-lg" href="/Ubicacion/Index" role="button">Cancelar</a>
        </div>



    </form>




</div>