@model PañaleraValentino.Models.VentaXVendedores
@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Crear</h2>

<div>
    <form action="/VentaXVendedores/Guardar" method="post">
        <div class="form-group">
            <label for="numventa">numventa: </label>
            <input type="text" class="form-control" name="numventa" id="numventa" />
        </div>
        <div class="form-group">
            <label for="idvendedor">IdVendedor: </label>
            <input type="text" class="form-control" name="IdVendedor" id="IdVendedor" />

        </div>

        <div class="form-group">
            <label for="idventa">IdVenta: </label>
            <input type="text" class="form-control" name="IdVenta" id="IdVenta" />

        </div>

        <div class="form-group">
            <label for="fechadeventa">FechaDeVenta: </label>
            <input type="text" class="form-control" name="FechaDeVenta" id="FechaDeVenta" />

        </div>

       

        <div class="form-group">
            <p><button class="btn btn-primary btn-lg" type="submit" role="button">Guardar</button></p>
        </div>
    </form>

</div>
